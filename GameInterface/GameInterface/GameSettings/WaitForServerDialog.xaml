<Window x:Class="GameInterface.GameSettings.WaitForServerDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameInterface.GameSettings"
        mc:Ignorable="d"
        Title="WaitForServerDialog" Height="140" Width="800" Loaded="Window_Loaded">
    <Grid Margin="10">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="ProgressText" Text="ゲーム開始まで : {hoge}" FontSize="32" Foreground="Gray" VerticalAlignment="Top" Grid.Row="0"/>
        <ProgressBar x:Name="Progress" Value="100" Grid.Row="2" Margin="10"/>
    </Grid>
</Window>
